programa
{
	
	funcao inicio()
	{
		exibirResultado(5.0, 3.0)
	}
	funcao real calcularArea(real largura, real altura){
		retorne largura * altura
	}
	funcao exibirResultado(real largura, real altura){
		real area = calcularArea(largura, altura)
		escreva("a area do retangulo é " ,area)
	}
}
