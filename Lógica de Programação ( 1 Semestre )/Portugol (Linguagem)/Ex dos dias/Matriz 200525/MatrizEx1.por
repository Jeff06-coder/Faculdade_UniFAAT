programa
{
	
	funcao inicio()
	{
	inteiro A[3][3]
		para (inteiro linha = 0; linha < 3; linha++){
			para (inteiro coluna = 0; coluna < 3; coluna++){
				escreva("Ovalor na posiçao [" ,linha, "][" ,coluna, "]: " ,A[linha][coluna], "\t")
			}
			escreva("\n")
		}
	}
}
