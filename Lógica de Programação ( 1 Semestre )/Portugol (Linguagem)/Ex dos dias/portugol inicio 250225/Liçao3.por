programa
{
	
	funcao inicio()
	{
		real largura, frente, result
		escreva ("Qual é a largura do terreno? ")
		leia (largura)
		escreva ("Qual é a comprimento (frente) do terreno? ")
		leia (frente)
		result = (largura * frente)
		escreva ("A área deste terreno é: " + result)
		
	}
}
