programa
{
	
	funcao inicio()
	{
		real grauC, fire
		escreva ("Qantos graus Celsius você deseja converter em Fahrenheit? ")
		leia (grauC)
		fire = (grauC * 9/5) + 32
		escreva ("A quantidade de Fahrenheit é " + fire + "")
	}
}
