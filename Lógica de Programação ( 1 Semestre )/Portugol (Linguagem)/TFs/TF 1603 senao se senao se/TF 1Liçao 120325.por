programa
{
	
	funcao inicio()
	{
		inteiro a, b
		escreva ("Escreva os números para ordenar-se: ")
		leia (a, b)
		se (a < b) {
			escreva ("A ordem crescente é: " ,a, "-" ,b)
		} senao {
			escreva ("A ordem crescente é: " ,b, "-" ,a)
		}
	}
}
